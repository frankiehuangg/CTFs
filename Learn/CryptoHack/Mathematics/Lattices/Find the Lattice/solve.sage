#!/usr/bin/sage

def gaussian(v1, v2):
	while (True):
		if (v2.norm() < v1.norm()):
			v1, v2 = v2, v1

		m = round((v1 * v2) / (v1.norm()^2))

		if (m == 0):
			return v1, v2
		
		v2 = v2 - m * v1

